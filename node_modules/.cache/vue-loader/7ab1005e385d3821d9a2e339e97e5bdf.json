t defineComponent({\n  name: 'App',\n  components: { JobList },\n  setup() {\n    const jobs = ref<Job[]>([\n      { title: 'Fullstack developer ', location: 'Surat', salary: 30000, id: '1' },\n      { title: 'Frontend developer', location: 'Surat', salary: 40000, id: '2' },\n      { title: 'Backend developer', location: 'Surat', salary: 35000, id: '3' },\n      { title: 'HR manager', location: 'Delhi', salary: 21000, id: '4' },\n      { title: 'prison guard', location: 'Ahemdabad', salary: 32000, id: '5' }\n    ])\n    const order = ref<OrderTerm>('title')\n    console.log('order',order);\n    \n\n    const handleClick = (term: OrderTerm) => {\n      order.value = term\n      console.log(\"handleClick\",term);\n    }\n    \n    return { jobs, order, handleClick }\n    \n  }\n});\n</script>\n\n<style>\n  header {\n    text-align: center;\n  }\n  header .order {\n    margin-top: 20px;\n  }\n  button {\n    margin: 0 10px;\n    color: #2e2e2e;\n    border: 3px solid #16ce93;\n    background-color: #005aff2e;\n    padding: 8px 16px;\n    border-radius: 4px;\n    cursor: pointer;\n    font-weight: bold;\n    margin-top: 6px;\n\n  }\n  @media (min-width: 320px) and (max-width: 481px) {\n    .order {\n   display: grid;\n  }\n}\n  header .title{\n    display: flex;\n    justify-content: center;\n  }\n  header img {\n    width: 60px;\n    margin-right: 20px;\n  }\n  header h1 {\n    font-size: 3em;\n  }\n</style>\n"]}]}